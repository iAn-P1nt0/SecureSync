{
  "name": "securesync",
  "version": "1.0.0",
  "description": "Intelligent dependency security scanner with auto-fix",
  "bin": {
    "securesync": "./dist/cli.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "security",
    "vulnerability",
    "scanner",
    "dependencies",
    "npm",
    "audit",
    "auto-fix",
    "breaking-changes",
    "migration"
  ],
  "author": "Ian Pinto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iAn-P1nt0/SecureSync.git"
  },
  "bugs": {
    "url": "https://github.com/iAn-P1nt0/SecureSync/issues"
  },
  "homepage": "https://github.com/iAn-P1nt0/SecureSync#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CLAUDE.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "inquirer": "^9.2.12",
    "semver": "^7.5.4",
    "pacote": "^17.0.4",
    "npm-registry-fetch": "^16.1.0",
    "fast-diff": "^1.3.0",
    "js-yaml": "^4.1.0",
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "^7.23.6",
    "@babel/generator": "^7.23.6",
    "@babel/types": "^7.23.6"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "tsup": "^8.0.1",
    "vitest": "^1.1.0",
    "@types/node": "^20.10.6",
    "@types/semver": "^7.5.6",
    "@types/js-yaml": "^4.0.9",
    "@types/babel__traverse": "^7.20.5",
    "@types/babel__generator": "^7.6.8",
    "prettier": "^3.1.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitest/coverage-v8": "^1.1.0"
  }
}
